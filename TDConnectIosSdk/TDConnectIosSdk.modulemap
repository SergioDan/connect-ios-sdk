framework module TDConnectIosSdk {
  umbrella header "TDConnectIosSdk.h"

  export *
  module * { export * }

  explicit module SysHeaders {
    // Explicetely define some system headers included by libcurl to prevent the “non-modular header inside framework module”
    header "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/clang/include/inttypes.h"
  }
}
